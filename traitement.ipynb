    "# On retire la chaîne de caractères \"nom_prenom\" car elle nous intéresse pas\n",
    "text_names_replaced = text_names_replaced.replace('nom_prenom', '')\n",
    "\n",
    "# Mise en minuscule\n",
    "text_lower = text_names_replaced.lower()\n",
